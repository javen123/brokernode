{
  "name": "brokernode",
  "version": "1.0.0",
  "description": "NodeJS and PHP code for the broker node.",
  "main": "index.js",
  "scripts": {
    "start": "cd testApi && nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrations": "cd peer-db && ../node_modules/.bin/sequelize db:migrate",
    "undoLastMigration": "cd peer-db && ../node_modules/.bin/sequelize db:migrate:undo",
    "undoAllMigrations": "cd peer-db && ../node_modules/.bin/sequelize db:migrate:undo:all",
    "seeders": "cd peer-db && ../node_modules/.bin/sequelize db:seed:all",
    "undoLastSeeder": "cd peer-db && ../node_modules/.bin/sequelize db:seed:undo",
    "undoAllSeeders": "cd peer-db && ../node_modules/.bin/sequelize db:seed:undo:all"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
