    "mariasql": "^0.2.6",
    "mongoose": "^5.0.7",
    "morgan": "~1.9.0",
    "mysql2": "^1.5.2",
    "pug": "2.0.0-beta11",
    "sequelize": "^4.34.0",
    "sequelize-cli": "^4.0.0",
    "serve-favicon": "~2.4.5",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "nodemon": "^1.15.1"
